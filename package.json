{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node .output/server/index.mjs",
    "build": "nuxt build && prisma generate",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "generate:prisma": "prisma generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && prisma generate",
    "lint": "eslint --ext .js,.ts,.vue  --ignore-path  .gitignore .",
    "lint:fix": "eslint --ext .js,.ts,.vue  --ignore-path  .gitignore --fix .",
    "prettify": "prettier  --ignore-path .gitignore .",
    "prettify:fix": "prettier  --ignore-path .gitignore --write .",
    "format": "pnpm lint && pnpm prettify",
    "format:fix": "pnpm lint:fix && pnpm prettify:fix"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-vue": "^9.19.2",
    "nuxt": "^3.9.1",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "sass": "^1.69.7",
    "vue": "^3.4.5",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "@hebilicious/vue-query-nuxt": "^0.3.0",
    "@prisma/client": "^5.7.1",
    "@tanstack/vue-query": "^5.17.7",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "zod": "^3.22.4"
  }
}
